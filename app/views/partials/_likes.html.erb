<% if tweet.voted_on_by? current_user %>
  <%= link_to '', unlike_tweet_path(tweet.id), remote: true, id: "like_#{tweet.id}", class: "glyphicon glyphicon-heart" %>
<% else %>
  <%= link_to '', like_tweet_path(tweet.id), remote: true, id: "like_#{tweet.id}", class: "glyphicon glyphicon-heart-empty" %>
<% end %>
<div class="likes" id="likes_<%= tweet.id %>">
  <%= display_likes tweet %>
</div>